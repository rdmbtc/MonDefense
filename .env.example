# MonFarm Environment Variables Template
# Copy this file to .env and fill in your actual values

# Privy Configuration (Required)
NEXT_PUBLIC_PRIVY_APP_ID=your-privy-app-id-here

# WalletConnect Configuration (Required for wallet connections)
# Get your project ID from https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your-walletconnect-project-id-here




# Blockchain Configuration (Optional - defaults provided)
NEXT_PUBLIC_MONAD_RPC_URL=https://testnet-rpc.monad.xyz
NEXT_PUBLIC_MONAD_CHAIN_ID=10143

# Server-side Wallet Configuration (Required for score submission)
# This wallet must have GAME_ROLE permission on the contract
GAME_WALLET_PRIVATE_KEY=your-game-wallet-private-key-here

# Development Configuration
NODE_ENV=development
